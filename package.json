{
  "name": "mqtt-hooks",
  "version": "1.0.9",
  "description": "ReactJS library for Pub/Sub communication with an MQTT broker using Hooks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "typings/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "typings"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mqtt",
    "react",
    "hooks",
    "context",
    "esp8266",
    "esp32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almond-hydroponics/mqtt-hooks.git"
  },
  "homepage": "https://github.com/almond-hydroponics/mqtt-hooks#readme",
  "author": "Francis Masha <francismasha96@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:start": "rollup -c -w",
    "prebuild": "rimraf dist",
    "prepublish": "yarn build",
    "build": "rollup -c",
    "test": "jest --maxWorkers=2",
    "test:unit": "jest --maxWorkers=2",
    "test:coverage": "yarn test:unit --coverage",
    "eslint": "eslint --report-unused-disable-directives --format codeframe '**/*.{js,jsx,ts,tsx,json}'",
    "eslint:fix": "yarn eslint --fix",
    "tslint": "tsc --noEmit && eslint \"*/**/*.{js,ts,tsx}\" --quiet",
    "lint:ts": "eslint --ext=jsx,ts,tsx lib"
  },
  "dependencies": {
    "duplexify": "^4.1.1",
    "mqtt": "4.2.1",
    "mqtt-pattern": "^1.2.0",
    "np": "^6.5.0",
    "react": "17.0.0-rc.1",
    "react-dom": "17.0.0-rc.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/dom": "^7.26.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/dot-object": "^2.1.2",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.3",
    "@types/testing-library__react-hooks": "^3.4.1",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "dot-object": "^2.1.4",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.2",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "safe-buffer": "^5.2.1",
    "sinon": "^9.2.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.6",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "yup": "^0.28.1"
  },
  "peerDependencies": {
    "yup": ">=0.29.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn prettier && yarn tslint"
    }
  }
}
