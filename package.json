  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mqtt",
    "react",
    "hooks",
    "context",
    "esp8266",
    "esp32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorHAS/mqtt-react-hooks.git"
  },
